{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "city": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "addedAt": {
      "type": "date"
    },
    "active": {
      "type": "boolean"
    },
    "seller": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "properties",
        "auto",
        "electronics",
        "clothes",
        "toys",
        "garden "
      ]
    }
  }
}
